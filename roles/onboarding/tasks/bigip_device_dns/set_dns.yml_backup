- name: Set the DNS settings on the BIG-IP
  bigip_device_dns:
    user: "{{ username }}"
    password: "{{ password }}"
    server: "{{ inventory_hostname }}"
    name_servers:
        - 8.8.8.8
        - 8.8.4.4.
    search:
        - localdomain
        - lab.local
    validate_certs: "no"
  delegate_to: localhost
